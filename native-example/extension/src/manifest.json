{
    "name": "Chrome Native Example",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.ts"
    },
    "content_scripts": [{
        "js": ["content-script.ts"],
        "matches": ["<all_urls>"]
    }],
    "action": {},
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "externally_connectable": {
        "matches": ["https://*.loca.lt/*", "*://localhost/*"]
    }
}
